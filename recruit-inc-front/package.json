{
  "name": "recruit-inc-front",
  "version": "1.0.0",
  "description": "Recruit Platform",
  "main": "webpack.config.js",
  "scripts": {
    "test": "tsc && jest",
    "build": "tsc && webpack --config webpack.config.js",
    "start": "npm run build && ts-node ./src/server.ts",
    "demo": "npm run start | lt --port 3000 --subdomain recruit-front ",
    "prettify": "prettier --single-quote --trailing-comma es5 --write \"*(src)/**/*.ts\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-merge": "npm install"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md,html,ts,tsx}": [
      "prettier --single-quote --trailing-comma es5 --write",
      "git add"
    ]
  },
  "jest": {
    "automock": false,
    "setupFiles": [
      "<rootDir>/test-shim.js",
      "<rootDir>/test-setup.js",
      "<rootDir>/setupJest.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/test-preprocessor.js"
    },
    "testMatch": [
      "**/test/**/*.(ts|tsx)"
    ]
  },
  "keywords": [],
  "author": "SOEN 490 - Team Abada",
  "license": "ISC",
  "devDependencies": {
    "@types/enzyme-adapter-react-16": "1.0.5",
    "@types/enzyme": "3.9.0",
    "@types/jest": "23.3.14",
    "@types/node": "10.12.27",
    "@types/react": "16.8.4",
    "@types/react-dom": "16.8.2",
    "@types/react-js-pagination": "3.0.2",
    "css-loader": "1.0.1",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.9.1",
    "file-loader": "2.0.0",
    "html-webpack-plugin": "3.2.0",
    "husky": "1.1.2",
    "jest": "24.1.0",
    "jest-fetch-mock": "2.1.1",
    "prettier": "1.15.3",
    "react-test-renderer": "16.8.3",
    "source-map-loader": "0.2.4",
    "style-loader": "0.23.1",
    "ts-loader": "4.5.0",
    "typescript": "3.3.3333",
    "url-loader": "1.1.2",
    "webpack": "4.8.3",
    "terser": "3.14.1",
    "webpack-cli": "3.2.0",
    "webpack-dev-middleware": "3.1.3",
    "webpack-hot-middleware": "2.22.2"
  },
  "dependencies": {
    "@material-ui/core": "3.9.2",
    "@material-ui/icons": "3.0.2",
    "@types/react-autosuggest": "9.3.7",
    "@types/react-select": "2.0.13",
    "awesome-typescript-loader": "5.2.1",
    "dotenv": "6.2.0",
    "dotenv-webpack": "1.7.0",
    "express": "4.16.4",
    "fflip": "4.0.0",
    "axios": "^0.18.0",
    "font-awesome": "^4.7.0",
    "fs": "0.0.1-security",
    "lint-staged": "8.1.4",
    "localtunnel": "1.9.1",
    "react": "16.8.3",
    "react-autosuggest": "9.4.3",
    "react-dom": "16.8.3",
    "react-js-pagination": "3.0.2",
    "react-select": "2.4.1",
    "ts-node": "7.0.1",
    "winston": "3.2.1"
  }
}
